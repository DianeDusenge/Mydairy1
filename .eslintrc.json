{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style":"off",
        "indent":"off",
        "prefer-const":"off",
        "arrow-parens":"off",
        "prefer-spread":"off",
        "no-unused-expressions":"off",
        "comma-dangle":"off",
        "no-else-return":"off",
        "semi":"off",
        "eol-last":"off",
        "prefer-arrow-callback":"off",
        "space-before-function-paren":"off",
        "radix":"off",
        "arrow-body-style":"off",
        "no-undef":"off",
        "quotes":"off",
        "no-console":"off",
        "func-names":"off"


    }
}